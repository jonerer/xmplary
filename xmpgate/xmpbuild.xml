<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project name="xmpbuild" default="create_run_jar"> 
    <!--this file is pretty awesome                                -->
    <!--ANT 1.7 is required                                        -->
	<target name="setup" description="Set up the individual project settings">
		<property name="certname" value="XMPlary_Gateway_Generous" />
		<property name="buildfolder" value="build/" />
		<property name="entry" value="se.lolcalhost.xmplary.xmpgate.GatewayMain" />

		<property name="name" value="gateway" />
		<property name="type" value="gateway" />
		<property name="FullName" value="XMPlary node gateway" />
		<property name="database" value="database.db" />
	</target>
	
	<import file="../xmpcommon/files/common_ant.xml" as="nested"/>

    <target name="create_run_jar" depends="nested.common_stuff">
    </target>
</project>
